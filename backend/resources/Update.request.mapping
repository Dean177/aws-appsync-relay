{
    "version" : "2017-02-28",
    "operation" : "UpdateItem",
    "key" : {
        "id": $util.dynamodb.toDynamoDBJson($ctx.args.input.id),
    },
    "update": {
     "expression" : "SET complete = :c",
     "expressionValues" : {
            ":c" : $util.dynamodb.toDynamoDBJson($ctx.args.input.complete)
        }
    },
    "condition" : {
      "expression"       : "userId = :sub",
      "expressionValues" : {
          ":sub" : { "S" : "${context.identity.sub}" }
      }
    }
}
